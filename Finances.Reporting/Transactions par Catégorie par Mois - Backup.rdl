      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select DATEPART(year, [date]) as year, DATEPART(month, [date]) as month, Etat, catégorie, SUM(total) as total
from [Transaction]
group by DATEPART(year, [date]), DATEPART(month, [date]), Etat, catégorie
order by DATEPART(year, [date]), DATEPART(month, [date]), Etat, catégorie</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
